\begin_inset Formula \( x\in \left[ 0,1\right] . \)
\end_inset 

 
\begin_inset Formula \begin{eqnarray*}
f\left( x;c\right)  & = & \left\{ \begin{array}{ccc}
2\frac{x}{c} &  & x<c\\
2\frac{1-x}{1-c} &  & x\geq c
\end{array}\right. \\
F\left( x;c\right)  & = & \left\{ \begin{array}{ccc}
\frac{x^{2}}{c} &  & x<c\\
\frac{x^{2}-2x+c}{c-1} &  & x\geq c
\end{array}\right. \\
G\left( q;c\right)  & = & \left\{ \begin{array}{ccc}
\sqrt{cq} &  & q<c\\
1-\sqrt{\left( 1-c\right) \left( 1-q\right) } &  & q\geq c
\end{array}\right. 
\end{eqnarray*}

\end_inset 


\begin_inset Formula \begin{eqnarray*}
\mu  & = & \frac{c}{3}\\
\mu _{2} & = & \frac{1-c+c^{2}}{18}\\
\gamma _{1} & = & \frac{\sqrt{2}\left( 2c-1\right) \left( c+1\right) \left( c-2\right) }{5\left( 1-c+c^{2}\right) ^{3/2}}\\
\gamma _{2} & = & -\frac{3}{5}
\end{eqnarray*}

\end_inset 

 
